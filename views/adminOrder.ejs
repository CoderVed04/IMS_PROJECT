<!doctype html>
<html>
    <head>
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">        </head>
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <body>
            <nav>
                <div class="nav-wrapper blue">
                  <a href="index" class="brand-logo">IMS</a>
                  <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li><a href="adminmaterials">Materials</a></li>
                    <li><a href="adminsuppliers">Suppliers</a></li>
                    <li class="active"><a href="adminorders">Orders</a></li>
                    <li><a href="users">Users</a></li>
                  </ul>
                </div>
              </nav>
              <div class= "container"><br>
                    <!-- <a href="addorder" class="btn blue" >add order</a> -->
                    <table class="striped">
                            <thead>
                              <tr>
                                  <th>Person Name</th>
                                  <th>Material</th>
                                  <th>Price</th>
                                  <th>Quantity</th>
                                  <th>Status</th>
                                  <th>created on</th>
                                  <th></th>
                              </tr>
                            </thead>                    
                            <tbody>
                              <% if (obj && obj.length) { %>
                              <% obj.forEach(function(object) { %>
                              <tr>
                                  <td><%= object.name %></td>
                                  <td><%= object.material %></td>
                                  <td>Rs&nbsp;<%= object.price %></td>
                                  <td><%= object.qty %></td>
                                  <td><%= object.status %></td>
                                  <td><%= object.created_on %></td>
                                  <!-- <td>
                                      <form action="/editorder/<%= object._id %>" method="GET">
                                          <input type="submit" class="btn green" value="edit">
                                      </form>
                                  </td> -->
                                  <!-- <td>
                                      <form action="/deleteorder/<%= object._id %>" method="post">
                                          <input type="submit" class="btn red" value="del">
                                      </form>
                                  </td> -->
                                  <td>
                                      <form action="/adminshoworder/<%= object._id %>" method="post">
                                          <input type="submit" class="btn blue" value="watch">
                                      </form>
                                  </td>
                              </tr>
                              <% }); %>
                              <% } else { %>
                              <tr>
                                  <td colspan="7">No Orders available</td>
                              </tr>
                              <% } %>
                          </tbody>
                          
                          </table><br><br>                  
                 </div>
                  <script
  src="https://code.jquery.com/jquery-3.3.1.min.js"
  integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
  crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>    </body>
</html>